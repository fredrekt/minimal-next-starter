{
  "name": "nextapp",
  "description": "A Minimalist Design of a Next.js Starter. Made with Base Web(baseui) and Material Design Bootstrap.",
  "author": "Fred Garingo <fredrickjohng7@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "baseui": "^9.99.0",
    "mdbreact": "^4.27.0",
    "next": "9.5.3",
    "nprogress": "^0.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-progressive-image": "^0.6.0",
    "styletron-engine-atomic": "^1.4.6",
    "styletron-react": "^5.2.7"
  },
  "devDependencies": {
    "eslint-plugin-baseui": "^9.99.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "baseui"
    ],
    "rules": {
      "baseui/deprecated-theme-api": "warn",
      "baseui/deprecated-component-api": "warn",
      "baseui/no-deep-imports": "warn"
    }
  }
}
